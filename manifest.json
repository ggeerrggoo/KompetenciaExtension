{
  "name": "A tehetség kapujának kulcsa",
  "description": "nagyon unalmas a kompetenciamérés, az automatizálása annál érdekesebb",
  "version": "0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js":["main.js"],
      "matches": [
        "https://www.tehetsegkapu.hu/*",
        "https://meres.tehetsegkapu.hu/*",
        "https://www.meres.tehetsegkapu.hu/*"
      ]
    }
  ]
}